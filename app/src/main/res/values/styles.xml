<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Material">
        <item name="android:colorPrimary">#9C29B5</item>
        <item name="android:colorPrimaryDark">#8A249F</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>

    <style name="NoActionBar" parent="android:Theme.Material">
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:colorPrimary">#363365</item>
        <item name="android:colorPrimaryDark">#363365</item>
    </style>

</resources>
